@model IEnumerable<ERA_BCMS.Models.ImageVideoDrawing>

@{
    ViewBag.Title = "ImageVideoDrawing";
}

<table class="table table-condensed">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BudgetYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PictureType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PictureId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PicDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PicDescr)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PicFileName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.BudgetYear)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PictureType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PictureId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PicDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PicDescr)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PicFileName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BridgeImageId }) |
            @Html.ActionLink("Details", "Details", new { id=item.BridgeImageId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BridgeImageId })
        </td>
    </tr>
}

</table>
