
<br />
<br />
<table class="table table-striped table-condensed table-responsive table-bordered display compact hover" id="tblDuplicateSectionNo">
    <tr>
        <th>
            Section Id
        </th>
        <th>
            Section No
        </th>
        <th>
            Section Name
        </th>
        <th>
            District
        </th>
        <th></th>
    </tr>

    @foreach (var duplcateSection in ViewBag.DuplicatesList)
    {

        foreach (var Section in duplcateSection)
        {
            <tr>
                <td>
                    @Section.SectionId
                </td>
                <td>
                    @Section.SectionNo
                </td>
                <td>
                    @Section.SectionName
                </td>
                <td>
                    @Section.District.DistrictName
                </td>
                <td align="center">
                    <a href="@Url.Action("Edit", "Section", new { id = Section.SectionId })" target="_blank" class="glyphicon glyphicon-new-window" data-toggle="tooltip" title="Go to Edit" style="font-size: 14px"></a>
                </td>
            </tr>
        }
        <tr style="background-color: lightgray">
            <td colspan="5"></td>
        </tr>
    }
</table>