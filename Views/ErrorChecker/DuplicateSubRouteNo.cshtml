
<br />
<br />
<table class="table table-striped table-condensed table-responsive table-bordered display compact hover" id="tblDuplicateSubRouteNo">
    <tr>
        <th>
            SubRoute Id
        </th>
        <th>
            SubRoute No
        </th>
        <th>
            SubRoute Name
        </th>
        <th>
            Main Route
        </th>
        <th></th>
    </tr>

    @foreach (var duplcateSubRoute in ViewBag.DuplicatesList)
    {

        foreach (var SubRoute in duplcateSubRoute)
        {
            <tr>
                <td>
                    @SubRoute.SubRouteId
                </td>
                <td>
                    @SubRoute.SubRouteNo
                </td>
                <td>
                    @SubRoute.SubRouteName
                </td>
                <td>
                    @SubRoute.MainRoute.MainRouteNo - @SubRoute.MainRoute.MainRouteName
                </td>
                <td align="center">
                    <a href="@Url.Action("EditSubRoute", "Routes", new { area = "Admin", id = SubRoute.SubRouteId })" target="_blank" class="glyphicon glyphicon-new-window" data-toggle="tooltip" title="Go to Edit" style="font-size: 14px"></a>
                </td>
            </tr>
        }
        <tr style="background-color: lightgray">
            <td colspan="5"></td>
        </tr>
    }
</table>