
<br />
<br />
<table class="table table-striped table-condensed table-responsive table-bordered display compact hover" id="tblDuplicateMainRouteNo">
    <tr>
        <th>
            MainRoute Id
        </th>
        <th>
            MainRoute No
        </th>
        <th>
            MainRoute Name
        </th>
        <th></th>
    </tr>

    @foreach (var duplcateMainRoute in ViewBag.DuplicatesList)
    {

        foreach (var MainRoute in duplcateMainRoute)
        {
            <tr>
                <td>
                    @MainRoute.MainRouteId
                </td>
                <td>
                    @MainRoute.MainRouteNo
                </td>
                <td>
                    @MainRoute.MainRouteName
                </td>
                <td align="center">
                    <a href="@Url.Action("EditMainRoute", "Routes", new { area = "Admin", id = MainRoute.MainRouteId })" target="_blank" class="glyphicon glyphicon-new-window" data-toggle="tooltip" title="Go to Edit" style="font-size: 14px"></a>
                </td>
            </tr>
        }
        <tr style="background-color: lightgray">
            <td colspan="4"></td>
        </tr>
    }
</table>
